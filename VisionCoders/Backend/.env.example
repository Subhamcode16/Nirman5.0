# Environment Variables

# OpenRouter API (Primary LLM Provider)
OPENROUTER_API_KEY=your_openrouter_api_key_here
OPENROUTER_CHAT_MODEL=openai/gpt-4o-mini
OPENROUTER_EMBEDDING_MODEL=openai/text-embedding-3-small

# Gemini AI (DEPRECATED - No longer used due to quota issues)
# GEMINI_API_KEY=your_key_here

# Supabase

# App Configuration
NODE_ENV=development
NEXT_PUBLIC_API_URL=http://localhost:3000

# File Upload
MAX_FILE_SIZE=10485760
ALLOWED_FILE_TYPES=application/pdf

# RAG Configuration
EMBEDDING_MODEL=embedding-001
LLM_MODEL=gemini-1.5-pro
CHUNK_SIZE=500
CHUNK_OVERLAP=50
TOP_K_RESULTS=5
SIMILARITY_THRESHOLD=0.7

# Rate Limiting
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=60000

# Python PDF Processor (optional - will fallback to pdf-parse if not available)
PDF_PROCESSOR_URL=http://localhost:8000
